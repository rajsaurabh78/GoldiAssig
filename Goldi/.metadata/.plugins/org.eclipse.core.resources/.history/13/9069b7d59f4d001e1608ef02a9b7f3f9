package Q1;

public class Main {
	    public static void main(String[] args){
	        String x="0";
	        check(x);
	    }
	    public static void check(String x){
	        int N = Integer.parseInt(x);
	        String temp="";
	        while(N>0){
	            temp+=N%2;
	            N=N/2;
	        }
	        String res="";
	        for(int i=temp.length()-1;i>=0;i--){
	            res+=temp.charAt(i);
	        }
	        
	        System.out.println(res);
	    }
}
